<?xml version="1.0" encoding="Windows-1252" ?>
<NotepadPlus>
	<AutoComplete language="NINMAP">
	<Environment ignoreCase="no" startFunc="(" stopFunc=")" paramSeparator="," terminal=";" additionalWordChar=""/>
		<KeyWord name="attribute" func="yes">
			<Overload retVal="void" descr="level attributes, MUST BE CALLED ONCE PER LEVEL\nAll args are Uint8">
			<Param name="Width" />
			<Param name="Height" />
			<Param name="RED" />
			<Param name="GREEN" />
			<Param name="BLUE" />
			<Param name="SpawnX" />
			<Param name="SpawnY" />
			<Param name="SongName" />
			</Overload>
		</KeyWord>
		<KeyWord name="background" func="yes">
			<Overload retVal="void" descr="background object, complete with movement and parallax">
			<Param name="Texture" />
			<Param name="X" />
			<Param name="Y" />
			<Param name="Depth" />
			<Param name="float XVel" />
			<Param name="float YVel" />
			<Param name="bool tileX" />
			<Param name="bool tileY" />
			</Overload>
		</KeyWord>
		<KeyWord name="beachhill" />
		<KeyWord name="beanstalk" />
		<KeyWord name="block" func="yes">
			<Overload retVal="void" descr="Texure can be name or Uint8 number">
			<Param name="Texture" />
			<Param name="X" />
			<Param name="Y" />
			<Param name="W" />
			<Param name="H" />
			</Overload>
		</KeyWord>
